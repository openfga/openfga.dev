{
  "name": "openfga.dev",
  "version": "0.0.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "dev": "docusaurus start --port=${PORT:-3000}",
    "docusaurus": "docusaurus",
    "start": "npm run serve --port=${PORT:-3000}",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
