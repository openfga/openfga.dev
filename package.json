  "repository": {
    "type": "git",
    "url": "git://github.com:openfga/openfga.dev"
  },
  "bugs": {
    "url": "https://github.com/openfga/openfga.dev/issues"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
